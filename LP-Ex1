x = open("L0Q1.in", 'r')
vez = x.readline()
vezes = int(vez)
for a in range (1, (vezes+1)):
    linha = x.readline() #cria uma string para cada linha do arquivo
    #com os espaços
    sem_espaco = linha.split(" ") #tira o espaço da string e salva
    #numa lista
    a = int(sem_espaco[0])
    c = int(sem_espaco[1])
    for b in range (0,c):
        if (a*b)%c==1:
            print b
        if (c%a==0):
            print "muito dificil"
x.close()    
y = open("L0Q1.out", 'w')

